{
    "name": "dogeswap",
    "description": "The DogeSwap DEX monorepo",
    "private": true,
    "workspaces": [
        "packages/*"
    ],
    "repository": "git@github.com:dogeswap-org/dogeswap.git",
    "keywords": [
        "dogeswap",
        "ethereum"
    ],
    "packageManager": "yarn@3.2.2",
    "scripts": {
        "dev": "concurrently \"yarn workspace @dogeswap/interface localnet:start\" \"yarn workspace @dogeswap/interface localnet:deploy\" \"yarn workspace @dogeswap/interface dev\""
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^5.25.0",
        "@typescript-eslint/parser": "^5.25.0",
        "concurrently": "^7.3.0",
        "eslint": "^8.16.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-react": "^7.30.0",
        "eslint-plugin-react-hooks": "^4.5.0",
        "prettier": "^2.7.1"
    },
    "resolutions": {
        "@types/react": "16.9.43",
        "@types/styled-components": "5.1.1"
    }
}