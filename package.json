{
    "name": "dogeswap",
    "description": "The DogeSwap DEX monorepo",
    "private": true,
    "workspaces": [
        "packages/*"
    ],
    "repository": "git@github.com:dogeswap-org/dogeswap.git",
    "keywords": [
        "dogeswap",
        "ethereum"
    ],
    "packageManager": "yarn@3.2.2",
    "scripts": {
        "clean": "yarn workspaces foreach run clean && hardhat clean",
        "dev": "yarn workspaces foreach run build && concurrently \"yarn localnet:run\" \"yarn localnet:deploy\" \"yarn workspace @dogeswap/interface dev\"",
        "localnet:run": "yarn hardhat node",
        "localnet:deploy": "yarn hardhat run --network localhost scripts/run-localnet.ts",
        "testnet:deploy": "yarn hardhat deploy --network testnet"
    },
    "devDependencies": {
        "@nomiclabs/hardhat-ethers": "^2.1.0",
        "@types/yargs": "^17.0.10",
        "@typescript-eslint/eslint-plugin": "^5.25.0",
        "@typescript-eslint/parser": "^5.25.0",
        "concurrently": "^7.3.0",
        "eslint": "^8.16.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-react": "^7.30.0",
        "eslint-plugin-react-hooks": "^4.5.0",
        "ethers": "^5.6.9",
        "hardhat": "^2.10.0",
        "prettier": "^2.7.1",
        "yargs": "^17.5.1"
    },
    "resolutions": {
        "@types/react": "16.9.43",
        "@types/styled-components": "5.1.1"
    }
}
