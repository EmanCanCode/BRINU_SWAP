{
    "extends": "../../tsconfig.json",
    "include": [
        "./src"
    ],
    "exclude": [
        "dist",
        "node_modules"
    ],
    "compilerOptions": {
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "jsx": "react",
        "module": "commonjs",
        "outDir": "/dist",
        "resolveJsonModule": true,
        "strict": true,
        "target": "es2021"
    }
}